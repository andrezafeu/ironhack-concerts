<h1>Add a new concert</h1>

<%=  form_for @my_concert do |f| %>
	<%= f.label :artist %>
	<%=f.text_field :artist %>
	<br><br>
	<%= f.label :venue %>
	<%= f.text_field :venue %>
	<br><br>
	<%= f.label :city %>
	<%= f.text_field :city %>
	<br><br>
	<%= f.label :date %>
	<%= f.datetime_local_field :date %>
	<br><br>
	<%= f.label :price %>
	<%= f.number_field :price %>
	<br><br>
	<%= f.label :description %>
	<%= f.text_area :description %>
	<%= f.button %>
<% end %>